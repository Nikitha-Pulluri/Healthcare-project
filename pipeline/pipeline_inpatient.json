{
	"name": "pipeline_inpatient",
	"properties": {
		"activities": [
			{
				"name": "Copy_inpatient_data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "ParquetSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "ParquetWriteSettings"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "CMSsqlserver",
						"type": "DatasetReference",
						"parameters": {
							"tableName": "inpatient_claims"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "CMSdatasink",
						"type": "DatasetReference",
						"parameters": {
							"folderName": "inpatient",
							"fileName": "inpatient_claims.parquet"
						}
					}
				]
			}
		],
		"annotations": []
	}
}